.page
  background: $grey200

.wrapper
  wrapper()

.container
  container()
  padding-top: 1px

.tabs
  position: relative
  margin-top: 84px
  cf()

.tab
  position: absolute
  opacity: 0
  transition: all .2s ease-out
  left: 0
  top: 0
  z-index: 1
  width: 100%

  &.active
    position: relative
    opacity: 1
    z-index 2

.text
  margin-bottom: 55px

.gallery
  spanFluid(4)

.text
  font-size: 14px
  line-height: 1.8
  spanFluid(8)

  p
    margin: 0 0 35px

.pricelist
  margin-bottom: 50px
  cf()

  &_image
    display: block
    margin: 0 auto
    max-width: 100%

.fos
  width: 50%
  margin: 0 auto
  cf()